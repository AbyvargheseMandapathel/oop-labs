import java.util.Scanner;

public class findsmallestandlargest {
    public static void main(String[] args) {

        System.out.println("enter the limit of array");

        Scanner s=new Scanner(System.in) ;
        int n = s.nextInt();
       int[] array= new int[n];
        System.out.println("ente rthe values in the array");
        for (int j=0; j<n;j++) {
            array[j]=s.nextInt();
        }
        int largest;
        int smallest;
        largest=array[0];
        smallest=array[0];
               for(int j=0; j<n;j++) {
                   if (array[j] > largest) {
                       array[j] = largest;
                   } else {

                       int temp = largest;
                       largest=temp;
                   }
                   for (j = 0; j <= n; j++) {
                       if (array[j] < smallest) {
                           array[j] = smallest;
                       } else {

                           int temp1 = smallest;
                           smallest=temp1;
                       }
                   }


                   System.out.println(largest);
                       System.out.println(smallest);

               }



    }
}
