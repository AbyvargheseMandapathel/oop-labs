public class   multiplytwomatrix {
    public static void main(String[] args) {

        int i,j,k;
        int a[][]={{1,1,1},{2,2,2},{3,3,3}};
        int b[][]={{2,2,2},{3,3,3},{4,4,4}};
        int c[][]=new int[3][3];

        for(i=0;i<3;i++){
            for(j=0;j<3;j++){
                c[i][j]=0;
                for(k=0;k<3;k++) {
                    c[i][k] = a[i][k] * b[i][k];
                    System.out.println(+c[i][k]);
                }
            }
        }

    }
}
